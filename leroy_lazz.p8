--helper functions

function get_bit(x, y, bit)
 local map_x = cur_map_x
 local map_y = cur_map_y
 return fget(mget(16*map_x+x/8,16*map_y+y/8),bit)
end

function is_obstacle(x, y)
 return get_bit(x, y, 1)
end

function do_music()
 if new_song == true then
  new_song = false
  music(song)
 end
end

function get_fires()
 local count = 0
 for i=0, 63 do
  for j=0, 127 do
   if mget(i,j) == 17 then
    count += 1
   end
  end
 end
 return count
end

function get_enemies()
 local count = 0
 for i=0, 63 do
  for j=0, 127 do
   if mget(i,j) >= 11 and mget(i,j) <=14 then
    count += 1
   end
  end
 end
 return count
end

